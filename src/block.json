{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/alertblock",
	"version": "0.1.0",
	"title": "Alert Block",
	"category": "widgets",
	"icon": "smiley",
	"description": "Block to create custom Alert messages",
	"supports": {
		"html": false
	},
	"attributes": {
		"alertMessage": {
			"type": "string"
		},
		"alertClass": {
			"type": "string"
		}
	},
	"textdomain": "alertblock",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}